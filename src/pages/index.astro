        }

        .icon {
            margin-right: 10px;
        }

        img {
            width: 200px;
            height: 125px;
            display: block;
            margin: 10px auto; /* Center image with margin */
            border-radius: 8px; /* Add border-radius for rounded corners */
        }

        footer p {
